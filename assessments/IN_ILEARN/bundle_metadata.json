{
    "earthmover_config_file": "earthmover.yaml",
    "edfi_data_model_version_range": [">=0.3.0", "<0.4.0"],
    "assessment_year_validated_range": [">=2024", "<=2024"],
    "student_id_names": ["STN"],
    "input_files": {
        "INPUT_FILE": {"optional": "False",
                       "required_fields": ["student_unique_id", "file_dir", "overall_scale_score", "overall_proficiency_level", "college_and_career_readiness_indicator"],
                       "optional_fields": ["ILEARN Reported Lexile® Measure", "ILEARN Reported Quantile® Measure", "ilearn_reporting_category_1_performance", "ilearn_reporting_category_2_performance", "ilearn_reporting_category_3_performance", "ilearn_reporting_category_4_performance", "ilearn_reporting_category_5_performance"]}
                    },
    "params": {
        "INPUT_FILE": 
            {
                "description": "The assessment file to be processed.",
                "optional": "False"
            },
        "OUTPUT_DIR":
            {
                "description": "The directory to output the resulting jsonl files.",
                "optional": "True",
                "default": "./"
            },
        "STATE_FILE":
            {
                "description": "The file which maintains the tool state.",
                "optional": "True",
                "default": "./runs.csv"
            },
        "STUDENT_ID_NAME":
            {
                "description": "The student ID column from the assessment file to use as the studentUniqueId. This will only be used if the parameter 'COMPUTE_MATCH_RATES' == 'False' and the parameter 'STUDENT_ID_MATCH_RATES' is empty.",
                "optional": "True",
                "default": "STN"
            },
        "POSSIBLE_STUDENT_ID_COLUMNS":
            {
                "description": "The set of student ID columns from the assessment file to compare to Ed-Fi student IDs using the compute_match_rates package",
                "optional": "True",
                "default": "STN"
            }
    }
}