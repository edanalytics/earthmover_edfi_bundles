{% if school is not none and school | length %}
{
  "educationOrganizationAssociationTypeDescriptor": "uri://ed-fi.org/EducationOrganizationAssociationTypeDescriptor#Enrollment",
  "educationOrganizationReference": {
    "educationOrganizationId": {{school|int}}
  },
  "schoolYearTypeReference": {
    "schoolYear": {{school_year}}
  },
  "studentAssessmentReference": {
    "assessmentIdentifier": "{{assessmentIdentifier}}",
    "namespace": "{{namespace}}",
    "studentAssessmentIdentifier": "{{studentAssessmentIdentifier}}",
    "studentUniqueId": "{{student_unique_id|int}}"
  }
}
{% elif district is not none and district | length %}
{
  "educationOrganizationAssociationTypeDescriptor": "uri://ed-fi.org/EducationOrganizationAssociationTypeDescriptor#Enrollment",
  "educationOrganizationReference": {
    "educationOrganizationId": {{district | int}}
  },
  "schoolYearTypeReference": {
    "schoolYear": {{school_year}}
  },
  "studentAssessmentReference": {
    "assessmentIdentifier": "{{assessmentIdentifier}}",
    "namespace": "{{namespace}}",
    "studentAssessmentIdentifier": "{{studentAssessmentIdentifier}}",
    "studentUniqueId": "{{student_unique_id|int}}"
  }
}
{% endif %}
