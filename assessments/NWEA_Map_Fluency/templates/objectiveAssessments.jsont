{
  "assessmentItems": [],
  "assessmentReference": {
    "assessmentIdentifier": "NWEA-Map-Fluency",
    "namespace": "uri://www.nwea.org/mapFluency"
  },
  "description": "{{identification_code}}",
  "identificationCode": "{{identification_code}}",
  "learningObjectives": [],
  "learningStandards": [],
  "performanceLevels": [
    {% for i in range(reporting_methods.split(',') | length) %}
      
      {
        "assessmentReportingMethodDescriptor": "uri://www.nwea.org/mapFluency/AssessmentReportingMethodDescriptor#{{ reporting_methods.split(',')[i] }}",
        "performanceLevelDescriptor": "uri://www.nwea.org/mapFluency/PerformanceLevelDescriptor#{{ levels.split(',')[i] }}",
        "resultDatatypeTypeDescriptor": "uri://ed-fi.org/ResultDatatypeTypeDescriptor#Level"
      } {% if not loop.last %},{% endif %}
      
    {% endfor %}
  ],
  "scores": [
    {
      "assessmentReportingMethodDescriptor": "uri://www.nwea.org/mapFluency/AssessmentReportingMethodDescriptor#Required Score",
      "resultDatatypeTypeDescriptor": "uri://ed-fi.org/ResultDatatypeTypeDescriptor#Integer"
    }
  ]
}