{%- set possible_accommodations = [
  [Acc1, "Acc1"],
  [Acc2, "Acc2"],
  [Acc3, "Acc3"],
  [Acc4, "Acc4"],
  [Acc5, "Acc5"],
  [Acc6, "Acc6"],
  [AccW1, "AccW1"],
  [AccW2, "AccW2"],
  [AccW3, "AccW3"],
  [AccW4, "AccW4"],
  [AccW5, "AccW5"],
  [AccW6, "AccW6"],
